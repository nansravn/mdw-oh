{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adw-oh-adf"
		},
		"ADLS Gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS Gen2'"
		},
		"Cloud Sales DB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Cloud Sales DB'"
		},
		"Cloud Streaming DB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Cloud Streaming DB'"
		},
		"Fourth Coffee FS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Fourth Coffee FS'"
		},
		"Movies DB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Movies DB'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"ADLS Gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mdwohadlsgen2.dfs.core.windows.net"
		},
		"Fourth Coffee FS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Rentals"
		},
		"Fourth Coffee FS_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "openhackadmin"
		},
		"MDW Key Vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mdw-oh-keyvault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adw-oh-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS Gen2_accountKey": {
						"value": "[parameters('ADLS Gen2_accountKey')]"
					},
					"Movies DB_connectionString": {
						"value": "[parameters('Movies DB_connectionString')]"
					},
					"Cloud Sales DB_connectionString": {
						"value": "[parameters('Cloud Sales DB_connectionString')]"
					},
					"Cloud Streaming DB_connectionString": {
						"value": "[parameters('Cloud Streaming DB_connectionString')]"
					},
					"Fourth Coffee FS_password": {
						"value": "[parameters('Fourth Coffee FS_password')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"ADLS Gen2_properties_typeProperties_url": {
						"value": "[parameters('ADLS Gen2_properties_typeProperties_url')]"
					},
					"MDW Key Vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('MDW Key Vault_properties_typeProperties_baseUrl')]"
					},
					"Fourth Coffee FS_properties_typeProperties_host": {
						"value": "[parameters('Fourth Coffee FS_properties_typeProperties_host')]"
					},
					"Fourth Coffee FS_properties_typeProperties_userId": {
						"value": "[parameters('Fourth Coffee FS_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adw-oh-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adw-oh-adf_ArmTemplate_0')]"
			]
		}
	]
}